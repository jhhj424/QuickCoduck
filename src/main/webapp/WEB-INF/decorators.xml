<?xml version="1.0" encoding="UTF-8" ?>
<decorators defaultdir="/decorator">
	<decorator name="main" page="main_decorator.jsp">
		<pattern>*.duck</pattern>
	</decorator>
	<excludes>
		<pattern>/user/start*</pattern>
		<pattern>*ajax_*</pattern>
		<pattern>/board/find.*</pattern>
		<pattern>*completeuserlist*</pattern>
	</excludes>
	<decorator name="mypage" page="mypage_decorator.jsp">
		<pattern>/user/mypage*</pattern>
		<pattern>/user/myduck*</pattern>
		<pattern>/admin/*</pattern>
	</decorator>
</decorators>