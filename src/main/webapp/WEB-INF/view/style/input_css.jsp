<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<style>
@import url("https://fonts.googleapis.com/css?family=Roboto:300,400,500");
input{
  width: 260px;
  height: 30px;
  padding-left: 10px;
  padding-top: 3px;
  padding-bottom: 3px;
  margin: 7px;
  font-size: 18px;
  border-radius: 10px;
  background: #6978762e;
  border: none;
  transition: background 0.5s;
}
input:focus {
  outline-width: 0;
  background: #0096884f;
  transition: background 0.5s;
}
input:hover{
  outline-width: 0;
  background: #0096884f;
  transition: background 0.5s;
}
</style>
</head>
</html>